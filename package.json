{
  "name": "@form8ion/eslint-config-hal",
  "description": "Shareable config for producers and consumers of HAL APIs",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/eslint-config-hal",
  "bugs": "https://github.com/form8ion/eslint-config-hal/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-hal",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:js": "eslint .",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.76",
    "@form8ion/eslint-config": "7.0.9",
    "@form8ion/remark-lint-preset": "6.0.3",
    "ban-sensitive-files": "1.10.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.11",
    "npm-run-all2": "6.1.2",
    "publint": "0.2.7",
    "remark-cli": "12.0.0"
  }
}
